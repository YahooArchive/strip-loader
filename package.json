{
  "name": "strip-loader",
  "version": "0.1.2",
  "description": "Webpack loader to strip arbitrary functions out of your production code.",
  "main": "lib/index.js",
  "scripts": {
    "cover": "node node_modules/istanbul/lib/cli.js cover --dir artifacts -- ./node_modules/mocha/bin/_mocha tests/unit/ --recursive --reporter spec",
    "lint": "./node_modules/.bin/jshint lib tests",
    "test": "mocha tests/unit --recursive --reporter spec"
  },
  "keywords": [
    "webpack",
    "strip"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:yahoo/strip-loader"
  },
  "author": "Rajiv Tirumalareddy <rajivtirum@yahoo-inc.com>",
  "devDependencies": {
    "chai": "^1.10.0",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.5",
    "jshint": "^2.5.11",
    "mocha": "^2.1.0",
    "webpack": "^1.4.15"
  },
  "dependencies": {
    "loader-utils": "^0.2.6"
  },
  "licenses": [
    {
      "type": "BSD",
      "url": "https://github.com/yahoo/strip-loader/blob/master/LICENSE.md"
    }
  ],
  "jshintConfig": {
    "node": true
  }
}
